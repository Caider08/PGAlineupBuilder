@{ 
    ViewData["Title"] = "Daily Fantasy Golf Articles";
}
@section MetaData
{
    <meta name="description" content="Daily Fantasy Golf advanced analysis and helpful tools including a LineupBuilder for Draft Kings, FanDuel, and FantasyDraft." />
    <meta name="keywords" content="DraftKings lineup builder, Fantasy Sports, Daily Fantasy Golf, PGA Golf, Draft Kings Daily Fantasy Golf, FanDuel Daily Fantasy Golf, Draft Kings projected ownership, fantasy golf analysis, fantasy pga," />
}
@model IList<PGAlineupBuilder.Models.BlogPost>
<div class="body-content">
    <div>
        <h1>Latest Daily Fantasy Golf Articles</h1>
    </div>

    @*<div class="tools-right">
        @Html.Partial("_SearchArticles")
    </div>*@

    @if (Model.Count() > 0)
    {
        foreach (BlogPost bp in Model)
        {
         
            @Html.Partial("_BlogPostTemplate", bp)

        }

    }
    else
    {
        <p>No Articles found!</p>

    }

    <div class="tools-left">

    </div>

</div>
