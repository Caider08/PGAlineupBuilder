@{
}

@model List<PGAlineupBuilder.Models.DkTourney>

<form method="post" asp-action="DisplayDK" asp-controller="BuildLineups">
    <select name="DKselected">
        @foreach (var tourney in Model)
        {
            <option value="@tourney.Name">@tourney.Name</option>
        }
    </select>
    <input type="submit" value="Build Lineups" />
</form>

@if (Model.Count == 0)
{
    <p>No Draft Kings Tournaments uploaded yet!</p>
}