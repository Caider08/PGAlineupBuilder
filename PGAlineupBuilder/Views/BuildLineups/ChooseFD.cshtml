@{
}
@model List<PGAlineupBuilder.Models.FDtourney>
<div style="margin-top: 25px">
<form method="post" asp-action="getFD" asp-controller="BuildLineups">
    <select name="FDselected">
        @foreach (var tourney in Model)
        {
            <option value="@tourney.Name">@tourney.Name</option>
        }
    </select>
    <input class="btn btn-default siteButtons" type="submit" value="Build Lineups" />
</form>

@if (Model.Count == 0)
{
    <p>No FanDuel Tournaments uploaded yet!</p>
}
</div>