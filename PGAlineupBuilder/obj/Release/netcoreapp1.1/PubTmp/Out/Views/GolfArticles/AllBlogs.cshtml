@{
}
@model IList<PGAlineupBuilder.Models.BlogPost>
<h2>All PGA Golf Posts</h2>
<div>
    @foreach(BlogPost post in Model)
    {
        <span><a asp-controler="GolfArticles" asp-action="GetBlogPost" asp-route-id="@post.ID">@post.Name</a></span>
    }

</div>

